		"webpack-dev-server": "^4.8.1",
		"webpack-merge": "^5.8.0"
	},
	"engines": {
		"node": "^12 || >=14"
	},
	"private": true,
	"repository": {
		"type": "git",
		"url": "git.riverstart.ru/rs/html/frontend-webpack-pug.git"
	},
	"scripts": {
		"dev": "webpack serve --config configuration/webpack.dev.config.js --mode=development --progress=profile",
		"build": "webpack --config configuration/webpack.prod.config.js --mode=production --progress=profile"
	},
	"target": "web",
	"dependencies": {
		"focus-visible": "^5.2.0"
